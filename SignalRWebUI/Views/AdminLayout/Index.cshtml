
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
@await Component.InvokeAsync("_LayoutHeaderPartialComponent")
<body>
	<div class="wrapper">
		@await Component.InvokeAsync("_LayoutNavbarPartialComponent")
		@await Component.InvokeAsync("_LayoutSidebarPartialComponent")
		<div class="main-panel">
			@RenderBody()
			@await Component.InvokeAsync("_LayoutFooterPartialComponent")
		</div>
	</div>


	@await Component.InvokeAsync("_LayoutScriptPartialComponent")
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
	<script src="~/js/site.js" asp-append-version="true"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>
